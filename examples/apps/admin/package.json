    "apollo-link-batch-http": "^1.2.3",
    "theme": "^1.0.0",
    "react-hot-loader": "^4.3.5",
    "@webiny/app-admin": "^1.0.0",
    "@webiny/app": "^1.0.0",
    "@webiny/ui": "^1.0.0",
    "@webiny/app-page-builder": "^1.0.0",
    "@webiny/app-i18n": "^1.0.0",
    "@webiny/app-forms": "^1.0.0",
    "@webiny/app-cookie-policy": "^1.0.0",
    "@webiny/app-google-tag-manager": "^1.0.0",
    "@webiny/app-mailchimp": "^1.0.0",
    "@webiny/app-security": "^1.0.0",
    "@webiny/app-typeform": "^1.0.0",
    "@webiny/plugins": "^1.0.0",
    "react-router-dom": "^5.0.0",
    "react-apollo": "^2.5.8",
    "url-search-params-polyfill": "^5.1.0",
    "cross-fetch": "^3.0.2"
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.7",
    "node-sass": "^4.9.3",
    "react-scripts": "^3.0.1",
    "source-map-explorer": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "SKIP_PREFLIGHT_CHECK=true PORT=3001 rescripts start",
    "build:dev": "env-cmd -f .env.dev rescripts build",
    "build:prod": "env-cmd -f .env.prod rescripts build",
    "postbuild": "webpack --config ./handler/webpack.config.js",
    "test": "rescripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "svgo": {
    "plugins": {
      "removeViewBox": false
    }
  }
}
