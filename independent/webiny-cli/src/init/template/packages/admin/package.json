{
  "name": "admin",
  "version": "1.0.0",
  "homepage": "/admin",
  "dependencies": {
    "apollo-link-batch-http": "^1.2.3",
    "react-apollo": "^2.2.4",
    "webiny-admin": "^1",
    "webiny-app": "^1",
    "webiny-app-cms": "^1",
    "webiny-app-security": "^1",
    "webiny-integration-cookie-policy": "^1",
    "webiny-integration-google-tag-manager": "^1",
    "webiny-integration-mailchimp": "^1",
    "webiny-integration-typeform": "^1",
    "webiny-plugins": "^1",
    "webiny-ui": "^1"
  },
  "devDependencies": {
    "node-sass": "^4.9.3",
    "react-hot-loader": "^4.3.5",
    "react-scripts": "^2.1.3",
    "@rescripts/cli": "^0.0.10",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "PORT=3001 rescripts start",
    "build": "cross-env PUBLIC_URL=/admin rescripts build",
    "test": "rescripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "svgo": {
    "plugins": {
      "removeViewBox": false
    }
  }
}
