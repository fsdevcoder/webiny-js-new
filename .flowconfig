[ignore]
.*/examples/.*
.*/tests/.*
[include]
[libs]
packages/webiny-api/flow-typed/npm
packages/webiny-client/flow-typed/npm
packages/webiny-client-security/flow-typed/npm
packages/webiny-api-security/flow-typed/npm
packages/webiny-entity-mysql/flow-typed/npm
packages/webiny-file-storage/flow-typed
[lints]

[options]
module.file_ext=.js
module.file_ext=.jsx
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=build/node_modules
module.system.node.resolve_dirname=./src
esproposal.class_static_fields=enable
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
[strict]
