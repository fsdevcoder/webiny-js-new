    "webiny-ui": "0.0.0"
  },
  "devDependencies": {
    "@rescripts/cli": "^0.0.7",
    "node-sass": "^4.9.3",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "SKIP_PREFLIGHT_CHECK=true PORT=3001 rescripts start",
