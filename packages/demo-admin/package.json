{
  "name": "demo-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-link-batch-http": "^1.2.3",
    "demo-theme": "0.0.0",
    "react-apollo": "^2.2.4",
    "react-hot-loader": "^4.3.5",
    "webiny-admin": "0.0.0",
