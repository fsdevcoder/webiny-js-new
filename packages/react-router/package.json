{
  "name": "@webiny/react-router",
  "version": "2.0.2-next.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/webiny/webiny-js.git"
  },
  "description": "A wrapper around react-router-dom to add Webiny related features.",
  "contributors": [
    "Pavel Denisjuk <pavel@webiny.com>",
    "Sven Al Hamad <sven@webiny.com>",
    "Adrian Smijulj <adrian@webiny.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@webiny/plugins": "^3.0.1",
    "apollo-client": "^2.6.8",
    "react-apollo": "^3.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/react-router-dom": "^5.1.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./dist *.tsbuildinfo && babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\"",
    "watch": "babel src -d dist --source-maps --copy-files --extensions \".ts,.tsx\" --watch",
    "postbuild": "cp package.json LICENSE README.md dist/ && tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn build"
  }
}
