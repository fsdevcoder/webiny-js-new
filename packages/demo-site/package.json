{
  "name": "demo-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-link-batch-http": "^1.2.3",
    "demo-theme": "0.0.0",
    "react-hot-loader": "^4.3.5",
    "webiny-app": "0.0.0",
    "webiny-app-cms": "0.0.0",
    "webiny-plugins": "0.0.0"
  },
  "devDependencies": {
    "node-sass": "^4.9.3",
    "react-app-rewired": "^1.5.2",
    "react-scripts": "^2.0.5",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "SKIP_PREFLIGHT_CHECK=true PORT=3002 react-app-rewired start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "svgo": {
    "plugins": {
      "removeViewBox": false
    }
  }
}
