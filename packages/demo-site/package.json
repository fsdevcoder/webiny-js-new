    "webiny-app": "0.0.0",
    "webiny-app-cms": "0.0.0",
    "webiny-app-cookie-policy": "0.0.0",
    "webiny-app-google-tag-manager": "0.0.0",
    "webiny-app-mailchimp": "0.0.0",
    "webiny-app-typeform": "0.0.0",
    "webiny-plugins": "0.0.0",
    "react-apollo": "2.3.3",
    "react-router-dom": "^5.0.0",
