    "react-dom": "^16.6.0"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "cross-env SKIP_PREFLIGHT_CHECK=true INLINE_RUNTIME_CHUNK=false rescripts build",
    "start": "cross-env SKIP_PREFLIGHT_CHECK=true PORT=3002 rescripts start",
    "test": "rescripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  },
  "svgo": {
    "plugins": {
      "removeViewBox": false
    }
  }
}
