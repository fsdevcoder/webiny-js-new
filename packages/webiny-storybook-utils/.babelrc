{
  "presets": [
    "babel-preset-react"
  ],
  "plugins": [
    ["babel-plugin-transform-class-properties"],
    ["babel-plugin-transform-object-rest-spread", {"useBuiltIns": true}],
    ["babel-plugin-transform-builtin-extend", {"globals": ["Error"]}],
    ["babel-plugin-transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}

