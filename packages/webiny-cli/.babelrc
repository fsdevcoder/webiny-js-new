{
    "presets": [
        [
            "babel-preset-env",
            {
                "targets": {
                    "node": "8.10"
                }
            }
        ],
        "babel-preset-flow"
    ],
    "plugins": [
        ["babel-plugin-module-resolver", {
            "root": ["./src"]
        }],
        ["babel-plugin-transform-object-rest-spread", { "useBuiltIns": true }],
        ["babel-plugin-transform-builtin-extend", { "globals": ["Error"] }],
        ["babel-plugin-transform-runtime"],
        ["babel-plugin-dynamic-import-node"]
    ]
}
