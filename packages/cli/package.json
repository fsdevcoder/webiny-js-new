{
  "name": "@webiny/cli",
  "version": "0.3.7",
  "main": "index.js",
  "bin": {
    "webiny": "./index.js",
    "wby": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webiny/webiny-js.git"
  },
  "author": "Pavel Denisjuk <pavel@webiny.com>",
  "description": "A tool to bootstrap a Webiny project.",
  "dependencies": {
    "@webiny/tracking": "^0.1.5",
    "chalk": "^2.4.2",
    "crypto": "^1.0.1",
    "execa": "^2.0.5",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "load-json-file": "^6.2.0",
    "node-fetch": "^2.6.0",
    "uuid": "^3.3.3",
    "write-json-file": "^4.2.0",
    "yargs": "^14.0.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf ./lib && babel src -d dist --source-maps --copy-files --ignore=src/create/template --include-dotfiles",
