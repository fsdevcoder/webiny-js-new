    "postbuild": "cp package.json LICENSE README.md dist/",
    "prepublishOnly": "yarn build"
  },
  "adio": {
    "ignoreDirs": [
      "/src/create/template/",
      "/node_modules/",
      "/backup/",
      "/dist/"
