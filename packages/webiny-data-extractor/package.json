{
  "name": "webiny-data-extractor",
  "version": "0.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/webiny/webiny-js.git"
  },
  "description": "A small library for easy async data extraction, using dot and square brackets notation.",
  "contributors": [
    "Pavel Denisjuk <pavel@webiny.com>",
    "Sven Al Hamad <sven@webiny.com>",
    "Adrian Smijulj <adrian@webiny.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-plugin-lodash": "^3.3.4"
  },
  "scripts": {
    "build": "babel src -d ${DEST:-build} --source-maps --copy-files",
    "flow-copy-source": "flow-copy-source src ${DEST:-build}",
    "postbuild": "yarn flow-copy-source"
  },
  "depCheck": {
    "ignore": {
      "dependencies": [
        "@babel/runtime"
      ],
      "devDependencies": [
        "@babel/cli",
        "@babel/core",
        "babel-plugin-lodash"
      ]
    }
  }
}
